import java.util.*;

public class Main {
    public static void add(ArrayList<Object> arr,Double d,String s,Integer i,Character c)
    {
      arr.add(d);
      arr.add(c);
      arr.add(s);
      arr.add(i);
    }
    public static void main(String[] args) {
       ArrayList<Object>arr=new ArrayList<>();
       Double d=1.5;
       Integer i=2;
       String s="jone";
       Character c='v';
       add(arr,d,s,i,c);
       System.out.println(arr);
    }
}